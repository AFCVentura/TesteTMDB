<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Aula2Rogerio.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Aula2Rogerio"
    Shell.FlyoutBehavior="Flyout"
    Title="Aula2Rogerio">

    <ShellContent
Title="Grid"
ContentTemplate="{DataTemplate local:GridPage}"
Route="Grid" />
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />


</Shell>
